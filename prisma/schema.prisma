generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String?
  createdAt DateTime @default(now())
}

model Product {
  id         String    @id @default(cuid())
  name       String
  price      Int
  createdAt  DateTime  @default(now())
  updatedAt  DateTime  @updatedAt
}